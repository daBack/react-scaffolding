{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.11",
    "merge": "^1.2.1",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
